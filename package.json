    "eslint-plugin-jest": "latest",
    "eslint-plugin-react": "latest",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swiper": "^11.0.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
