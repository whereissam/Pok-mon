{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "jsx": "preserve",
    "strict": true,
    "allowJs": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node10",
    "outDir": "dist"
  },
  // "include": ["src"],
  "exclude": ["node_modules", "tmp"]
}
